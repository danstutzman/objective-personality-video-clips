{
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "./dist/",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "jsxFactory": "h",
    "allowJs": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["DOM", "ES2015"]
  }
}
